@model List<InfoPortalWeb.Models.FeedbackInfo>

@{
    ViewData["Title"] = "Feedback";
    var checkSession = ViewBag.checkSession;
}

<h1>Feedback</h1>

<table class="table">
    <thead>
        <tr>
            <th>Username</th>
            <th>Feedback Message</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var feedback in Model)
        {
            <tr>
                <td>@feedback.Username</td>
                <td>@feedback.FeedbackMessage</td>
            </tr>
        }
    </tbody>
</table>
<div><a href="@Url.Action("Index","Admin")" class="mt-3 btn btn-primary">Dashboard</a></div>